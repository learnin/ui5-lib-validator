{
    "_version": "1.17.0",
    "sap.app": {
      "id": "learnin.ui5.validator.example",
      "type": "application",
      "applicationVersion": {
        "version": "0.1.0"
      },
      "title": "Example of ui5-validator",
      "description": "Example application to showcase the consumption of ui5-validator."
    },
    "sap.ui": {
      "technology": "UI5",
      "deviceTypes": {
        "desktop": true,
        "tablet": true,
        "phone": true
      }
    },
    "sap.ui5": {
      "handleValidation": true,
        "rootView": {
          "viewName": "learnin.ui5.validator.example.view.ValidatorExample",
          "type": "XML",
          "async": true,
          "id": "app"
      },
      "dependencies": {
        "minUI5Version": "1.71.11",
        "libs": {
          "sap.m": {},
          "sap.ui.core": {}
        }
      },
      "contentDensities": {
        "compact": true,
        "cozy": true
      }
    }
  }
